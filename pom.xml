    <distributionManagement>
        <repository>
            <id>zebra-releases</id>
            <name>ali-releases</name>
            <url>http://139.196.16.147:8789/nexus/content/repositories/releases/</url>
        </repository>
        <snapshotRepository>
            <id>zebra-snapshots</id>
            <name>ali-snapshots</name>
            <url>http://139.196.16.147:8789/nexus/content/repositories/snapshots/</url>
        </snapshotRepository>
    </distributionManagement>
</project>
